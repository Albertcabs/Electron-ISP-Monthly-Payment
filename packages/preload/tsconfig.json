// packages/preload/tsconfig.json
{
   "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "isolatedModules": true,
      "lib": ["esnext", "dom"]
   },
   "include": [
      "./**/*.ts",
      "../renderer/src/vite-env.d.ts",
      "./xlsxFiles/*.js",
      "./xlsxFiles/*.xlsx",
      "./**/*.d.ts"
   ],
   "references": [{ "path": "../../tsconfig.node.json" }]
}
